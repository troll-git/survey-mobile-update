{
  "expo": {
    "name": "surveyapp",
    "slug": "surveyingapp-mobile",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*",
      "./assets/**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "ca.brentvatne.growlerprowler",
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      }
    },
    "description": ""
  }
}